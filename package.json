{
  "name": "server-template",
  "version": "1.0.0",
  "description": "A node server template.",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.58",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".gitignore",
      "node_modules/**/node_modules"
    ],
    "verbose": false,
    "execMap": {
      "ts": "ts-node"
    },
    "events": {
      "restart": "echo App restarted due to changes..."
    },
    "watch": [
      
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts,json"
  }
}
